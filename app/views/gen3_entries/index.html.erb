<p id="notice"><%= notice %></p>

<h1>Gen3 Entries</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @gen3_entries.each do |gen3_entry| %>
    <tr>
      <td><%= gen3_entry.player_name %></td>
      <td><%= link_to gen3_entry.save_file.filename, gen3_entry.save_file if gen3_entry.save_file.attached? %></td>
      <td><%= link_to 'Show', gen3_entry %></td>
      <td><%= link_to 'Edit', edit_gen3_entry_path(gen3_entry) %></td>
      <td><%= link_to 'Destroy', gen3_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gen3 Entry', new_gen3_entry_path %>
