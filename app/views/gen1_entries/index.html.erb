<p id="notice"><%= notice %></p>

<h1>Gen1 Entries</h1>

<table>
  <thead>
    <tr>
      <th>Playername</th>
      <th>Savefile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gen1_entries.each do |gen1_entry| %>
      <tr>
        <td><%= gen1_entry.playerName %></td>
        <td><%= gen1_entry.saveFile %></td>
        <td><%= link_to 'Show', gen1_entry %></td>
        <td><%= link_to 'Edit', edit_gen1_entry_path(gen1_entry) %></td>
        <td><%= link_to 'Destroy', gen1_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gen1 Entry', new_gen1_entry_path %>
